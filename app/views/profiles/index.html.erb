<p id="notice"><%= notice %></p>

<h1>Discover New Artist</h1>
<p>
<div class="row clearfix">
    <!-- Put the curated profiles here -->
    <% @profiles.each do |profile| %>

      <div class="row"> 
        <div class="col-sm-1">
          <div class="span">
            <%= link_to (image_tag profile.user.avatar.url(:thumb)), profile  %>
          </div>
        </div>
      </div>
    <% end %>
</div>


